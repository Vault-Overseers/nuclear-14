  .desc = { ent-N14TableCounter.desc }
ent-N14TableCounterCross = { ent-N14TableCounter }
  .suffix = перекрестие
  .desc = { ent-N14TableCounter.desc }
ent-N14TableCounterCraft = { ent-N14TableCounter }
  .desc = Эта стойка выглядит так, как будто её собрали из лома. Кустарное производство по-пустошному. 
  .suffix = полная поделка
ent-N14TableCounterCraftBend = { ent-N14TableCounter }
